{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "Pivotal": "Debug",
      "Steeltoe": "Debug"
    }
  },
  // begin - spring cloud eureka settings
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "validateCertificates": false
    },
    "instance": {
      "port": 8010,
      "preferIpAddress": true,
      "instanceId": "agent-service-container:8010"
    }
  },
  // end - spring cloud eureka settings
  // begin - spring cloud config settings
  "spring": {
    "application": {
      "name": "sampleservice-foo"         // 配置文件name
    }
  },
  "cloud": {
    "config": {
      "uri": "http://localhost:8888", // 配置中心uri
      "validateCertificates": false
    }
  },
  // end - spring cloud config settings
  "AllowedHosts": "*"
}
